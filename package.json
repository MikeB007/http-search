{
  "name": "news-search",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider\" ng serve",
    "build": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider\" ng build",
    "build:prod": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider\" ng build --configuration production",
    "watch": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider\" ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint || echo 'Lint not configured'",
    "serve:https": "node server.js",
    "serve:auto": "node start-application.js",
    "docker:build": "docker build -t http-search -f DOCKER/Dockerfile .",
    "docker:run": "docker-compose -f DOCKER/docker-compose.yml up --build",
    "docker:run:detached": "docker-compose -f DOCKER/docker-compose.yml up -d --build",
    "docker:dev": "docker-compose -f DOCKER/docker-compose.dev.yml up --build",
    "docker:stop": "docker-compose -f DOCKER/docker-compose.yml down",
    "docker:logs": "docker-compose -f DOCKER/docker-compose.yml logs -f",
    "docker:clean": "docker-compose -f DOCKER/docker-compose.yml down -v --rmi all",
    "setup:certs": "node scripts/setup-certificates.js",
    "setup:auto": "node scripts/setup-certificates.js && node server.js",
    "prod:start": "npm run setup:certs && PUBLIC_IP=147.194.240.208 INTERNAL_IP=192.168.86.40 node server.js",
    "prod:docker": "PUBLIC_IP=147.194.240.208 INTERNAL_IP=192.168.86.40 SSL_PASSPHRASE=production123 docker-compose -f DOCKER/docker-compose.yml up --build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.2",
    "@angular/cdk": "^12.0.4",
    "@angular/common": "~12.0.2",
    "@angular/compiler": "~12.0.2",
    "@angular/core": "~12.0.2",
    "@angular/forms": "~12.0.2",
    "@angular/material": "^12.0.4",
    "@angular/platform-browser": "~12.0.2",
    "@angular/platform-browser-dynamic": "~12.0.2",
    "@angular/router": "~12.0.2",
    "body-parser": "^1.20.3",
    "express": "^4.21.2",
    "rxjs": "~6.6.0",
    "s": "^1.0.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.2",
    "@angular/cli": "~12.0.2",
    "@angular/compiler-cli": "~12.0.2",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "cross-env": "^7.0.3",
    "jasmine-core": "~3.7.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.2.3"
  }
}
